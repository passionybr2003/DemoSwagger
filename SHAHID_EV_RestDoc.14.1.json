  schemas:
    createUser:
      type: object
      required:
        - channelPartnerID
        - apiKey
      properties:
        channelPartnerID:
          type: string
          description: Unique ID number of channel partner.
          enum:
            - SHAHID
        apiKey:
          type: string
          description: Partner authentication Key.
        socialLoginType:
          type: string
          description: Social login type for the user.
          enum:
            - Google
            - Facebook
            - Apple
        email:
          type: string
