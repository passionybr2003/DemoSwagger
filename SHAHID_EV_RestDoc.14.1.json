    post:
      security:
        - bearerAuth: []
      tags:
        - Order Processing
      operationId: AddSubscriptionRequestMessage
      description: >-
        addSubscription() REST API is used to place a subscription at a go to
        the customer.
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                properties:
                  AddSubscriptionResponseMessage:
                    type: object
