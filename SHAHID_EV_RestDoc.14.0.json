  /getPkgsAndProdsWithPromos:
    post:
      security:
        - bearerAuth: []
      tags:
        - Offer Management APIs
      operationId: PkgsOrProdsRequestMsg
      description: 'getPkgsAndProdsWithPromos() REST API is used to fetch the details of Products, Packages and Promotions that are associated with Products and Packages in the given DMA.'
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                properties:
                  PkgsOrProdsResponseMsg:
                    type: object
                    properties:
                      responseMessage:
                        type: string
                        description: SUCCESS/FAIL.
                      responseCode:
                        type: string
                        description: Indicates if the response was successfully processed. Accepted values are <br> “1” – Success <br> “0” – Failure.
                      pkgWithPromos:
                        type: array
                        items:
                          $ref: '#/components/schemas/pkgWithPromos'
                        description: Returns all packages with promos.
                      prodWithPromos:
                        type: array
                        items:
                          $ref: '#/components/schemas/prodWithPromos'
                        description: Returns all products with promos.
              examples:
                '111111111':
                  value:
                    PkgsOrProdsResponseMsg:
                      failureMessage:
                        - errorMessage: Authentication Failed
                          errorCode: '111111111'
                      responseCode: '0'
                Sample successful response:
                  value:
                    PkgsOrProdsResponseMsg:
                      message: SUCCESS
                      responseCode: '1'
                      pkgWithPromos:
                        - promoMsg:
                            duration: '1'
                            promotionName: Free trial 7 days
                            promotionType: Free
                            isSelfPromotion: 'true'
                            displayName: Monthly VIP Plan
                            dmaName: Nationwide_CMA
                            promotionID: 7DFTRIAL
                          pkgMsg:
                            displayName: Monthly Pack
                            chargeType: OneTime
                            dmaName: Nationwide_CMA
                            type: Prepaid
                            basicService: 'False'
                            packageName: Shahid Monthly Subscription
                            bbDescription: '2 streams. 5 devices. Unlimited streaming.'
                            retailPriceDate: 03/29/2021
                            disPlayOrder: '1'
                            retailPrice: '149.00'
