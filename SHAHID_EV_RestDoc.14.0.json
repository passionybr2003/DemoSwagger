  /getPaymentModes:
    post:
      tags:
        - Offer Management APIs
      operationId: PaymentModesRequestMsg
      description: getPaymentModes() REST API is used to get all the available Payment Modes
      responses:
        '200':
          description: Success
          content:
            application/json:
              schema:
                properties:
                  PaymentModesResponseMsg:
                    type: object
                    properties:
                      message:
                        type: string
                        description: Success/Fail.
                      responseCode:
                        type: string
                        description: >-
                          Indicates if the response was successfully processed.
                          Accepted values are <br> “1” – Success <br> “0” –
                          Failure.
                      paymentModesMsg:
                        type: array
                        items:
                          $ref: '#/components/schemas/paymentModesMsg'
                        description: Payment mode details.
              examples:
                '111111111':
                  value:
                    PaymentModesResponseMsg:
                      failureMessage:
                        - errorMessage: Authentication Failed
                          errorCode: '111111111'
                      responseCode: '0'
                Sample successful response with params:
                  value:
                    PaymentModesResponseMsg:
                      message: SUCCESS
                      responseCode: '1'
                      paymentModesMsg:
                        - paymentMode: Credit Card
                          paymentChannel: Credit Card
                          supportsAutoRenew: T
                          view_mode: Native
                          isPaymentGroup: T
                          paymentModeDescription: ICICI Credit Card
                          imageUrl: 'https://shahid.evergent.com/user/creditcard?creditcard'
                          group: CC
                          paymentModes:
                            - paymentMode: Credit Card
                              paymentChannel: Credit Card
                              supportsAutoRenew: T
                              view_mode: Native
                              url: 'https://shahid.evergent.com/shahid/operator.png'
                              paymentModeDescription: ICICI Credit Card
                        - paymentMode: Operator
                          paymentChannel: Operator Billing
                          supportsAutoRenew: T
                          view_mode: chromecustom
